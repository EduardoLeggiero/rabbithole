{
	"base_search_url": "http://h33t.com/search/",
	"headers":  
	{		
	},
	"page_rules":
	{
		"start":1,
		"step":1,
		"max_items":200
	},
	"pattern": "[query]/category/[category]/order_uploaded/page[page]",
	"operations":["titles_filter","category_filter","size_filter","seed_filter","leech_filter","url_filter"],
	"categories":
	{
		"All":0,
		"Movies":"Movies",
		"Music":"Music",		
		"Games":"Games",
		"Other":"Other",
		"Anime":"Anime",
		"Books":"Books",
		"Windows":"Windows",
		"Linux":"Linux",
		"Mac":"Mac",
		"Audio Books":"Audio-Books",
		"Television":"Television",
		"Comics":"Comics",
		"Music Video":"Music-Video",
		"Factual":"Factual",
		"Pictures":"Pictures",
		"Mobile Phone":"Mobile-Phone",
		"HD Anime":"HD-Anime",
		"HD Television":"HD-Television",
		"HD Movies":"HD-Movies",
		"HD Music Video":"HD-Music-Video",
		"Kindle":"Kindle",
		"Psp":"Psp"

	},
	"titles_filter": 
	{
		"tag": "td",
		"attrs":
		{
			"class":"lista",
			"align":"left"
		},
		"method": "text"
	},
	"category_filter":
	{
		"tag": "a",
		"attrs":
		{
			"href":true,
			"title":true
		},
		"method": "attr",
		"attr":"title",
		"attr_len":2,
		"chipper":
		{
			"start":29,
			"step":4
		},
		"manipulate":
		{
			"to_replace":["hot torrents"],
			"replace_with":[""]
		}
	},
	"size_filter":
	{
		"tag": "td",
		"attrs":
		{
			"class":"lista",
			"align":"center"
		},
		"method": "text",
		"attr_len":2,
		"chipper":
		{
			"start":4,
			"step":11
		}
	},
	"seed_filter":
	{
		"tag": "td",
		"attrs":
		{
			"align":"center",
			"class":"lista"
		},
		"method":"text",
		"attr_len":2,
		"chipper":
		{
			"start":7,
			"step":11
		}
	},
	"leech_filter":
	{
		"tag": "td",
		"attrs":
		{
			"align":"center",
			"class":"lista"
		},
		"method":"text",
		"attr_len":2,
		"chipper":
		{
			"start":6,
			"step":11
		}
	},
	"url_filter":
	{
		"tag": "a",
		"attrs":
		{ 
			"href": true,
			"title":"Torrent File"
		},
		"method": "attr",
		"attr":"href"
	}
}
