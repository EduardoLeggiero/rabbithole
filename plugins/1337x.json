{
    "base_search_url": "http://1337x.org/search/",
    "file_format":"gzip",
    "headers":{},
    "page_rules":
    {
        "start":0,
        "step":1,
	"max_items":20
    },
    "pattern": "[query]/[page]/[category]",
    "operations":["titles_filter","category_filter","size_filter","seed_filter","leech_filter","url_filter","magnet_url_filter"],
    "categories":
    {
        "All":""
    },
    "titles_filter": 
    {
        "tag": "h3",
	"attrs_len":1,
        "attrs":
        {
            "class":"org"
        },
        "method": "text",
        "manipulate":
        {
            "to_replace":["\n\n1"],
	    "replace_with":[""]
        },
	"chipper":
	{
	    "start":1,
	    "step":1
	}
	
    },
    "category_filter":
    {
        "tag": "a",
        "attrs":
                {
                    "href":true,
                    "class":true,
                    "title":true
                },
        "method": "attr",
        "attr":"title",
        "attrs_len":3
    },
    "size_filter":
    {
        "tag": "span",
        "attrs":
        {
            "class":"size"
        },
        "method": "text"
    },
    "seed_filter":
    {
        "tag": "span",
        "attrs":
        {
            "class":"seed"
        },
        "method":"text"
    },
        "leech_filter":
    {
        "tag": "span",
        "attrs":
        {
            "class":"leech"
        },
        "method":"text"
    },
    "url_filter":
    {
        "tag": "a",
        "attrs":
        { 
            "href": true,
            "class":"org"
        },
        "attr_len":2,
        "method": "attr",
        "attr":"href",
                "chipper":
        {
            "start":1,
            "step":1
        },
	"manipulate":
	{
	    "prefix": "http://1337x.org"
	},
	"crawler":
	{
	    "tag":"a",
	    "attrs":
	    {
		"class":"torrentDw",
		"title":"Torrent Download"
					},
	    "method":"attr",
	    "attr":"href"
	}
    },
    "magnet_url_filter":
    {
        "tag": "a",
        "attrs":
        { 
            "href": true,
            "class":"org"
        },
        "attr_len":2,
        "method": "attr",
        "attr":"href",
        "chipper":
        {
            "start":1,
            "step":1
        },
	"manipulate":
	{
	    "prefix": "http://1337x.org"
	},
	"crawler":
	{
	    "tag":"a",
	    "attrs":
	    {
		"class":"magnetDw",
		"title":"Magnet Download"
					},
	    "method":"attr",
	    "attr":"href"
	}
    }
}
